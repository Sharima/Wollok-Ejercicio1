object rick {
	var property acompaniante
	var property nivelDemencia
	var property irAventura
	
	method aumentaDemencia(cantidad){
		nivelDemencia =+ cantidad
	}
	
	method disminuyeDemencia(cantidad){
		nivelDemencia =- cantidad
	}
}

object morty {
	var property saludMental
	var property irAventura
	
	method irseDeAventura(){
		saludMental =- 30
		rick.aumentaDemencia(50)
	}
}

